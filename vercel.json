{
    "version": 2,
    "builds": [
      {
        "src": "flask_app/app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "flask_app/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "flask_app/app.py"
      }
    ],
    "env": {
      "PYTHONUSERBASE": "/vercel/workpath/.python",
      "PYTHONUNBUFFERED": "1",
      "VERCEL_ENV": "1",
      "VERCEL_URL": "amr-predict.vercel.app",
      "VERCEL_GIT_COMMIT_SHA": "d5b4fa89a287dd0ea",
      "VERCEL_GIT_REPO_SLUG": "microsoft/vscode",
      "VERCEL_GIT_PROVIDER": "github",
      "VERCEL_GIT_COMMIT_MESSAGE": "fix: run tasks",
      "VERCEL_GIT_COMMIT_REF": "main"
    }
  }
  